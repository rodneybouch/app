{
  "expo": {
    "name": "Unspoken",
    "slug": "unspoken",
    "scheme": "unspoken",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": { "image": "./assets/splash.png", "resizeMode": "contain", "backgroundColor": "#0f0f10" },
    "assetBundlePatterns": ["**/*"],
    "ios": { "supportsTablet": true },
    "android": {
      "adaptiveIcon": { "foregroundImage": "./assets/adaptive-icon.png", "backgroundColor": "#0f0f10" },
      "package": "com.unspoken.app",
      "permissions": ["SCHEDULE_EXACT_ALARM"],
      "versionCode": 1,
      "useNextNotificationsApi": true
    },
    "web": { "bundler": "metro", "favicon": "./assets/favicon.png" },
    "plugins": [["expo-notifications"], ["expo-sqlite"]]
  }
}
